                    d="M111.927,142.139c-3.914,-11.187 -7.471,-23.587 -9.432,-34.403c-1.683,-9.275 -2.14,-17.447 -0.923,-22.895c1.296,-5.81 4.436,-9.082 8.977,-9.62c4.105,-0.486 7.589,1.235 10.301,5.011c2.891,4.027 4.919,10.873 5.97,19.133c0.333,2.619 0.578,5.386 0.744,8.244c0.118,-6.134 0.879,-11.366 2.422,-14.914c1.698,-3.905 4.348,-6.111 7.634,-6.605c3.691,-0.556 6.87,1.589 8.663,6.714c2.11,6.031 2.435,17.281 1.521,29.606c-0.94,12.666 -3.126,26.485 -5.596,36.724c0.325,0.97 0.653,1.889 0.981,2.751c0.868,2.279 1.72,4.141 2.562,5.428c0.195,0.298 0.462,0.625 0.646,0.84c2.377,-0.749 4.127,-2.653 5.486,-4.834c2.32,-3.722 3.517,-8.31 4.073,-11.098c0.065,-0.918 0.222,-1.877 0.475,-2.879c0.31,-1.189 1.409,-2 2.637,-1.946c1.227,0.055 2.25,0.959 2.455,2.171c0.004,0.024 -0.024,0.418 -0.104,1.089l0.055,0.002c-0.041,0.152 -0.078,0.301 -0.111,0.449c-0.056,0.428 -0.13,0.933 -0.223,1.501c-0.196,3.009 1.276,5.233 3.885,6.794c4.256,2.546 10.959,3.586 18.573,3.646c16.607,0.13 37.456,-4.567 48.257,-10.855c0.909,-0.539 2.085,-0.239 2.624,0.67c0.539,0.909 0.239,2.085 -0.67,2.624c-13.166,7.953 -40.897,13.888 -58.993,11.785c-7.034,-0.817 -12.606,-2.897 -15.827,-6.299c-0.645,1.768 -1.444,3.554 -2.433,5.203c-2.234,3.725 -5.401,6.704 -9.599,7.675c-1.184,0.274 -2.488,0.039 -3.805,-0.955c-0.939,-0.707 -1.98,-1.97 -3.002,-3.761c-0.174,-0.306 -0.35,-0.631 -0.527,-0.973c-0.298,0.847 -0.595,1.622 -0.89,2.32c-1.063,2.51 -2.248,4.206 -3.242,5.077c-1.24,1.087 -2.532,1.447 -3.7,1.428c-1.073,-0.018 -2.455,-0.419 -3.87,-1.667c-1.148,-1.013 -2.611,-2.922 -4.176,-5.62c-0.79,-1.364 -1.632,-2.956 -2.508,-4.739c-4.041,9.514 -9.168,12.654 -13.323,12.817c-4.25,0.168 -8.212,-2.574 -10.587,-6.503c-2.301,-3.808 -3.04,-8.552 -1.585,-12.004c1.849,-4.39 5.402,-7.483 9.45,-9.178c2.478,-1.038 5.125,-1.551 7.636,-1.552c-1.486,-1.328 -3.008,-2.438 -4.555,-3.357c-12.209,-7.252 -25.7,-2.793 -34.466,-1.621c-1.047,0.149 -2.018,-0.579 -2.167,-1.625c-0.149,-1.047 0.579,-2.018 1.625,-2.167c9.452,-1.438 23.929,-6.069 37.268,1.